<config_plugin>
	<!-- configure here the additional metadata that shall be added to the anchor file or the volume part of the mets file -->
	<metadata value="CHANGE_ME" type="TitleDocMain" anchor="true" volume="false" />
	<metadata value="CHANGE_ME" type="CatalogIDSource" anchor="true" volume="false" />
	<metadata value="CHANGE_ME" type="CatalogIDDigital" anchor="true" volume="false" />
	
	<metadata value="CHANGE_ME" type="CurrentNoSorting" anchor="false" volume="true" />
	
	<!-- which folder to use as import source -->
	<importFolder>/opt/digiverso/import/sample/</importFolder>
	
	<!-- which workflow to use -->
	<workflow>Newspaper_workflow</workflow>
	
</config_plugin>
